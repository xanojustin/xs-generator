// DocuSign Send Envelope - Creates and sends a DocuSign envelope for e-signature
run.job "DocuSign Send Envelope" {
  main = {
    name: "send_envelope"
    input: {
      subject: "Please sign this document"
      signer_name: "Test Signer"
      signer_email: "signer@example.com"
      document_base64: ""
      document_name: "Sample Document.pdf"
      document_file_extension: "pdf"
    }
  }
  env = ["DOCUSIGN_ACCOUNT_ID", "DOCUSIGN_ACCESS_TOKEN"]
}
